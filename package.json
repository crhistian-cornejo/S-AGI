  "scripts": {
    "predev": "bun apps/electron/scripts/predev.ts",
    "dev": "electron-vite dev",
    "dev:web": "vite --config vite.config.ts",
    "build": "electron-vite build",
    "build:web": "vite build --config vite.config.ts",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "typecheck:core": "cd packages/core && bun run tsc --noEmit",
    "clean": "rm -rf out dist apps/electron/release",
    "clean:modules": "rm -rf node_modules apps/electron/node_modules packages/*/node_modules",
    "electron:dist": "bun run build && electron-builder --config apps/electron/electron-builder.yml --project apps/electron",
    "electron:dist:mac": "bun run build && electron-builder --config apps/electron/electron-builder.yml --project apps/electron --mac",
    "electron:dist:win": "bun run build && electron-builder --config apps/electron/electron-builder.yml --project apps/electron --win",
    "electron:dist:linux": "bun run build && electron-builder --config apps/electron/electron-builder.yml --project apps/electron --linux",
    "postinstall": "electron-rebuild -f -w better-sqlite3"
  },
